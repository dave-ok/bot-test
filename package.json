{
  "name": "test-bot",
  "version": "1.0.0",
  "description": "A twitter bot test",
  "main": "server.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint --fix \"src/**/*.js\"",
    "build": "babel src --out-dir dist --delete-dir-on-start --copy-files --ignore src/**/temp_models",
    "dev": "cross-env DEBUG=log NODE_ENV=development nodemon --exec babel-node ./src/index.js",
    "start": "cross-env DEBUG=log NODE_ENV=production node ./dist/index.js",
    "devstart": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dave-ok/bot-test.git"
  },
  "keywords": [
    "twitter",
    "bot"
  ],
  "author": "dave-ok",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dave-ok/bot-test/issues"
  },
  "homepage": "https://github.com/dave-ok/bot-test#readme",
  "dependencies": {
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ngrok": "^3.2.7",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "twit": "^2.2.11",
    "twitter-autohook": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
